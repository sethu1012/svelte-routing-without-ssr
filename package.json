{
  "name": "svelte-app",
  "version": "1.0.0",
  "devDependencies": {
    "rollup": "^1.31.1",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.2.0",
    "svelte": "^3.18.2"
  },
  "dependencies": {
    "sirv-cli": "^0.4.4",
    "svelte-routing": "^1.4.1"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --single",

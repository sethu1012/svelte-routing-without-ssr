{
  "name": "svelte-app",
  "version": "1.0.0",
  "devDependencies": {
    "rollup": "^2.38.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.31.2"
  },
  "dependencies": {
    "sirv-cli": "^1.0.10",
    "svelte-routing": "^1.5.0"
  },
  "scripts": {
